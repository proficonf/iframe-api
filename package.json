{
  "name": "@proficonf/iframe-api",
  "version": "0.1.0",
  "description": "Proficonf Embedded API",
  "main": "dist/index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "test": "BABEL_ENV=test ./node_modules/.bin/webpack --mode=development --target node --config webpack.config.js && jasmine dist/main.js",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --mode=production --config webpack.config.js",
    "serve": "NODE_ENV=development ./node_modules/.bin/webpack serve --config webpack.config.js",
    "lint": "eslint . --no-inline-config --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@babel/core": "7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "7.15.0",
    "@proficonf/eslint-config": "^1.0.0",
    "@proficonf/utils": "2.8.1",
    "babel-loader": "8.2.2",
    "eslint": "7.0.0",
    "glob": "7.1.7",
    "html-webpack-plugin": "4.5.2",
    "jasmine": "3.6.1",
    "nanoid": "3.1.23",
    "terser-webpack-plugin": "5.1.4",
    "webpack": "5.51.0",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proficonf/iframe-api.git"
  },
  "author": "Proficonf Ltd.",
  "license": "UNLICENSED",
  "homepage": "https://github.com/proficonf/iframe-api#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
